    420           * @param   port    - UART port
    421           *          event   - Event that causes the callback
    422           *
    423           *
    424           * @return  None
    425           ***************************************************************************************************/
    426          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    427          {
    428          
    429            osal_event_hdr_t  *msg_ptr;
    430            uint16 length = 0;
    431            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    432          
    433            /*
    434               If maxZAppBufferLength is 0 or larger than current length
    435               the entire length of the current buffer is returned.
    436            */
    437            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    438            {
    439              length = MT_UartMaxZAppBufLen;
    440            }
    441            else
    442            {
    443              length = rxBufLen;
    444            }
    445          
    446            /* Verify events */
    447            if (event == HAL_UART_TX_FULL)
    448            {
    449              // Do something when TX if full
    450              return;
    451            }
    452          
    453            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    454            {
    455              if ( App_TaskID )
    456              {
    457                /*
    458                   If Application is ready to receive and there is something
    459                   in the Rx buffer then send it up
    460                */
    461                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    462                {
    463                  /* Disable App flow control until it processes the current data */
    464                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    465          
    466                  /* 2 more bytes are added, 1 for CMD type, other for length */
    467                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    468                  if ( msg_ptr )
    469                  {
    470                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    471                    msg_ptr->status = length;
    472          
    473                    /* Read the data of Rx buffer */
    474                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    475          
    476                    /* Send the raw data to application...or where ever */
    477                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    478                  }
    479                }
    480              }
    481            }
    482          }
